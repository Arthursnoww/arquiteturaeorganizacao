**questão-02**

a)

   mov eax, total
   sar eax, 5
   mov total, eax
   mov eax, num
   sub eax, total
   mov answer, eax

b)

  mov eax, amount
  add eax, number
  sal eax, 2
  mov result, eax

c)

  mov eax, y
  sal eax, 3
  mov y, eax
  mov eax, z
  sar eax, 1
  add eax, y
  mov x, eax

d)

  mov eax, b
  sal eax, 2
  mov ebx, b
  sal ebx, 1
  add eax, ebx
  mov b, eax
  mov eax, a
  sar eax, 4
  sub eax, b
  mov a, eax
